{
  "name": "Google Inbox",
  "description": "Google Inbox as a standalone application.",
  "version": "0.2",
  "author": "Jake Bellacera",
  "manifest_version": 2,
  "permissions": [
    "webview",
    "browser"
  ],
  "app": {
    "background": {
      "scripts": ["main.js"]
    }
  },
  "icons": {
    "32": "icon-32.png",
    "96": "icon-96.png",
    "256": "icon-256.png"
  }
}
